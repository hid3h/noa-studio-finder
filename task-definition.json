{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/noa-rails-task",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "secrets": [
        {
          "valueFrom": "noa_rails_master_key",
          "name": "RAILS_MASTER_KEY"
        }
      ],
      "memory": null,
      "memoryReservation": 128,
      "volumesFrom": [],
      "image": "okikawaru",
      "essential": true,
      "name": "rails"
    }
  ],
  "memory": null,
  "taskRoleArn": null,
  "family": "noa-rails-task",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": null
}
